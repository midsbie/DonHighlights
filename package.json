{
  "name": "html-highlighter",
  "title": "Html Highlighter",
  "description": "Highlight and select phrases in HTML pages.",
  "version": "0.4.3",
  "main": "dist/htmlhighlighter.js",
  "module": "src/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dossier/html-highlighter.git"
  },
  "keywords": [
    "html",
    "highlighter"
  ],
  "author": "Diffeo, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dossier/html-highlighter.git/issues"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --color --display-error-details",
    "start": "webpack-dev-server --progress --color --hot --inline --host 0.0.0.0 --port $([ -z \"$NODE_PORT\" ] && echo 8080 || echo $NODE_PORT)",
    "upgrade": "yarn upgrade-interactive --latest",
    "check": "yarn run check:flow && yarn run check:lint && yarn run check:fmt && echo 'check: PASS'",
    "check:flow": "flow",
    "check:lint": "yarn run check:lint:src && yarn run check:lint:cfg",
    "check:lint:nocache": "eslint --format $(sh/get-eslint-formatter) --color --max-warnings=0 ./packages/*/*/src",
    "check:lint:cfg": "eslint --format $(sh/get-eslint-formatter) --color --cache --max-warnings=0 ./config/",
    "check:lint:src": "eslint --format $(sh/get-eslint-formatter) --color --cache --max-warnings=0 ./packages/*/*/src",
    "check:fmt": "yarn run check:fmt:js && yarn run check:fmt:json && yarn run check:fmt:md && yarn run check:fmt:css",
    "check:fmt:js": "prettier './packages/**/{e2e,src}/**/*.{js,jsx}' --check",
    "check:fmt:config": "prettier './packages/**/tasks/**/*.js' --check",
    "check:fmt:json": "prettier --parser json '**/package.json' --check",
    "check:fmt:md": "prettier --parser markdown '**/*.md' --check",
    "check:fmt:css": "prettier --parser css '**/*.css' --check",
    "check:fmt:scss": "prettier --parser scss '**/*.scss' --check",
    "fix:fmt": "yarn run fix:fmt:js; yarn run fix:fmt:config; yarn run fix:fmt:json; yarn run fix:fmt:md; yarn run fix:fmt:css",
    "fix:fmt:js": "yarn run check:fmt:js --write",
    "fix:fmt:config": "yarn run check:fmt:config --write",
    "fix:fmt:json": "yarn run check:fmt:json --write",
    "fix:fmt:md": "yarn run check:fmt:md --write",
    "fix:fmt:css": "yarn run check:fmt:css --write",
    "fix:fmt:scss": "yarn run check:fmt:scss --write"
  },
  "files": [
    "src/*",
    "dist/*"
  ],
  "dependencies": {
    "merge": "^1.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-flow-strip-types": "^7.7.4",
    "@babel/plugin-transform-regenerator": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.6",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.7.2",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-junit": "^1.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "flow-bin": "^0.113.0",
    "flow-typed": "^2.6.2",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "prettier": "^1.19.1",
    "terser-webpack-plugin": "^2.2.2",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-node-externals": "^1.7.2",
    "yarn": "^1.21.0"
  }
}
