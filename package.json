{
  "name": "don-highlights",
  "title": "Don Highlights",
  "description": "Versatile library enabling highlighting of arbitrary phrases in live web pages",
  "version": "1.0.1",
  "main": "lib/don-highlights.js",
  "unpkg": "dist/don-highlights.js",
  "module": "es/don-highlights.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/midsbie/don-highlights.git"
  },
  "publishConfig": {
    "registry": "https://npm.softgeist.com"
  },
  "keywords": ["dom", "web", "highlighting", "highlights", "highlighter"],
  "author": "Miguel Guedes <miguel.a.guedes@gmail.com> (https://github.com/miguel-guedes)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miguel-guedes/don-highlights.git/issues"
  },
  "scripts": {
    "upgrade": "yarn upgrade-interactive --latest",
    "check": "yarn run check:flow && yarn run check:lint && yarn run check:fmt && echo 'check: PASS'",
    "check:push": "yarn run check && yarn run clean && yarn run test && yarn run build",
    "clean": "rimraf coverage dist es lib junit.xml yarn-error.log",
    "test": "jest",
    "build": "rollup -c && yarn run build:flow",
    "build:flow": "echo \"// @flow\\n\\nexport * from '../src';\" > lib/don-highlights.js.flow",
    "watch": "rollup -c --watch",
    "check:flow": "flow",
    "check:lint": "yarn run check:lint:src",
    "check:lint:nocache": "eslint --format $(sh/get-eslint-formatter) --color --max-warnings=0 ./src",
    "check:lint:src": "eslint --format $(sh/get-eslint-formatter) --color --cache --max-warnings=0 ./src",
    "check:fmt": "yarn run check:fmt:js && yarn run check:fmt:json && yarn run check:fmt:md",
    "check:fmt:js": "prettier './src/**/*.js' --check",
    "check:fmt:json": "prettier --parser json '**/package.json' --check",
    "check:fmt:md": "prettier --parser markdown '**/*.md' --check",
    "fix:fmt": "yarn run fix:fmt:js; yarn run fix:fmt:json; yarn run fix:fmt:md",
    "fix:fmt:js": "yarn run check:fmt:js --write",
    "fix:fmt:json": "yarn run check:fmt:json --write",
    "fix:fmt:md": "yarn run check:fmt:md --write"
  },
  "files": ["dist/*", "es/*", "lib/*", "src/*"],
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "merge": "^1.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-flow-strip-types": "^7.7.4",
    "@babel/plugin-transform-regenerator": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.6",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "eslint": "^7.4.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-junit": "^1.0.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "flow-bin": "^0.129.0",
    "flow-typed": "^3.2.0",
    "jest": "^26.1.0",
    "jest-junit": "^11.0.1",
    "jsdom": "^16.3.0",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.21.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "yarn": "^1.22.4"
  }
}
